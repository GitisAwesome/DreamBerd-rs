{
  "highlight.regexFlags": "gi",
  "highlight.regexes": {
    "const (const|var) +(var|(?!\\1)const)": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "backgroundColor": "#ff4346", // this is to highlight an invalid usage of const const const
          "color": "#1f1f1f"
        },
        {}
      ]
    },
    "(const|var)(\\s+)(const|var)(\\s+)([^ +\\-\\*\\/<>=\\(\\)\\[\\]!;:\\.{}\n,]+)(:?\\s*)(([\\w<>\\[\\],]+?))?( *)([+\\-\\/*]?)(= *)([^!\n?]+)": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "color": "#66cc80" // first const
        },
        {},
        {
          "color": "#6680cc" // second const
        },
        {},
        {
          "color": "#ff8046" // variable name
        },
        {},
        {
          "color": "#ffa0de" //type
        }
      ]
    },
    "({)([^}]*)(})(€|円|₽)": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "color": "#6680cc"
        },
        {
          "color": "#ffffff"
        },
        {
          "color": "#6680cc"
        },
        {
          "color": "#dccc66"
        }
      ]
    },
    "(\\$|£|¥)({)([^}]*)(})": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "color": "#dccc66"
        },
        {
          "color": "#6680cc"
        },
        {
          "color": "#ffffff"
        },
        {
          "color": "#6680cc"
        }
      ]
    },
    "([\"'`])(.*)(\\1)": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "color": "#228840"
        },
        {
          "color": "#228840"
        },
        {
          "color": "#228840"
        }
      ]
    },
    "(„)(.*)(“)": {
      "filterFileRegex": ".*\\..db",
      "decorations": [
        { "color": "#228840" },
        { "color": "#228840" },
        { "color": "#228840" }
      ]
    },
    "(»)(.*)(«)": {
      "filterFileRegex": ".*\\..db",
      "decorations": [
        { "color": "#228840" },
        { "color": "#228840" },
        { "color": "#228840" }
      ]
    },
    "(«)(.*)(»)": {
      "filterFileRegex": ".*\\..db",
      "decorations": [
        { "color": "#228840" },
        { "color": "#228840" },
        { "color": "#228840" }
      ]
    },
    "([^ +\\-\\*\\/<>=\\(\\)\\[\\]!;:\\.{}\n,\"]+)(:\\s*)([\\w<>\\[\\],]+)(,|\\))": {
      "filterFileRegex": ".*\\.db",
      "decorations": [{ "color": "#ff8046" }, {}, { "color": "#ffa0de" }]
    },
    "([^ +\\-\\*\\/<>=\\(\\)\\[\\]!;:\\.{}\n,]+)(\\.)([^ +\\-\\*\\/<>=\\(\\)\\[\\]!;:\\.{}\n,]+)\\s*": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "color": "#6680cc" // object name
        },
        {},
        {
          "color": "#66cc80" // variable name
        }
      ]
    },
    "\\b(true|false|maybe|undefined|self|if|delete|eval)\\b": {
      "filterFileRegex": ".*\\.db",
      "decorations": [
        {
          "color": "#eeaa46"
        }
      ]
    }
  }
}
